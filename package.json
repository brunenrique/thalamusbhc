    "jest": "^30.0.2",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "node-fetch": "^3.3.2",
    "pa11y": "^9.0.0",
    "playwright": "^1.53.1",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.10",
